%\newfontfamily\lithuanianfonttt[Script=SSS,Lithuanian=LLL]{TTFONT}% from ttf-TTFONT

% Define quotation mark styles
%\setquotestyle[guillemets]{lithuanian}% Use guillemets for quotations
%\DeclareQuoteStyle[quotes]{lithuanian}
